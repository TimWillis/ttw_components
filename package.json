{
  "name": "ttw_components",
  "version": "1.0.7",
  "description": "\"# typescript web components\"",
  "main": "dist\\cjs\\index.js",
  "module": "dist\\index.js",
  "types": "dist\\index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "no_lint": "echo \"Error: no test specified\"",
    "build": "npx tsc -p tsconfig.json && npx tsc -p tsconfig_cjs.json && npx tsc  --project src/file_utilities/tsconfig.json && node dist/file_utilities/folder_copy.js && node dist/file_utilities/img_inliner.js",
    "dev": "concurrently --kill-others \"tsc -p tsconfig.json -w\" \"tsc -p tsconfig_cjs.json -w\"",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run no_lint",
    "preversion": "npm run no_lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimWillis/ttw_components.git"
  },
  "keywords": [
    "typescript",
    "web",
    "components"
  ],
  "author": "ttw",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TimWillis/ttw_components/issues"
  },
  "homepage": "https://github.com/TimWillis/ttw_components#readme",
  "dependencies": {
    "diff-dom": "^4.2.8",
    "flatpickr": "^4.6.13",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "fs-extra": "^11.1.0",
    "prettier": "^2.8.1"
  }
}
